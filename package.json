  "scripts": {
    "dev": "(webpack --mode=development) & node server",
    "start": "(webpack --mode=development) & node server",
    "build": "webpack --mode=production --config webpack.config.js",
    "update-android": "npx cap sync; npx cap open android",
    "update-ios": "npx cap sync; npx cap open ios",
    "build-android": "npm run build; npm run update-android",
    "build-ios": "npm run build; npm run update-ios",
    "dev-ios": "(webpack --mode=development); npm run update-ios",
    "dev-android": "(webpack --mode=development); npm run update-android",
    "watch-legacy": "(webpack --config webpack.config.js --watch) & (cd dist; python2 -m SimpleHTTPServer 9110)",
    "watch": "(webpack --mode=development --config webpack.config.js --watch) & (node server.js)"
  },
